	<entry key="dubboApplicationName">dubbo_application</entry>
	<entry key="dubboApplicationValue">blog_service</entry>
	
	<entry key="dubboProtocolName">dubbo_protocol</entry>
	<entry key="dubboProtocolValue">dubbo</entry>
	
	<entry key="dubboPortName">dubbo_port</entry>
	<entry key="dubbboPortValue">2881</entry>
	
	<!-- log -->
	<entry key="serviceLogRootName">service_log_root</entry>
	<entry key="serviceLogRootValue">/home/huoshan/log</entry>
	
	<entry key="serviceLogNameName">service_log_name</entry>
	<entry key="serviceLogNameValue">blog_service.log</entry>
	
	<entry key="serviceLogLevelName">service_log_level</entry>
	<entry key="serviceLogLevelValue">DEBUG</entry>
	
	<entry key="serviceExceptionLogNameName">service_exception_log</entry>
	<entry key="serviceExceptionLogNameValue">blog_service_exception.log</entry>
	
	<entry key="serviceSqlLogNameName">service_sql_log_name</entry>
	<entry key="serviceSqlLogNameValue">blog_service_sql.log</entry>
	
	<entry key="serviceActiveMqLogName">service_active_mq_log_name</entry>
	<entry key="serviceActiveMqLogNameValue">blog_service_activemq.log</entry>
	
	<!-- serviceimpl变量结束 -->
	
	<!-- web变量 -->
	<entry key="webFreemarkerDirName">blog_freemarker_dir</entry>
	<entry key="webFreemarkerDirValue"></entry>
	
	<entry key="webUploadMaxSizeName">blog_upload_maxsize</entry>
	<entry key="webUploadMaxSizeValue">800000000</entry>
	
	<entry key="webUploadErrorPageName">blog_upload_errorpage</entry>
	<entry key="webuploadErrorPageValue">error_page</entry>
	
	<entry key="webViewTemplateSuffixName">blog_view_template_suffix</entry>
	<entry key="webViewTemplateSuffixValue">.ftl</entry>
	
	
	<entry key="webLoggingRootName">blog_logging_root</entry>
	<entry key="webLoggingRootValue">/home/huoshan/log</entry>
	
	<entry key="webLoggingNameName">blog_logging_name</entry>
	<entry key="webLoggingNameValue">blog_web.log</entry>
	
	<entry key="webLoggingLevelName">blog_logging_level</entry>
	<entry key="webLoggingLevelValue">DEBUG</entry>
	
	<entry key="webDubboApplicationName">dubbo_application</entry>
	<entry key="webDubboApplicationValue">blog_web</entry>
	
	<!-- web变量结束 -->
	
	<!-- jsp namespace: web/${namespace}/${className}/list.jsp -->
	<entry key="namespace"></entry>

	<entry key="outRoot">/home/huoshan/generator-output</entry>
	
