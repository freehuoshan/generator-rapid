<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<!-- 使用zookper暴露发现服务地址 -->
	<!-- <dubbo:registry address="zookeeper://192.168.204.133:2181" check="true" /> -->
	 <dubbo:registry address="${}" check="true" />
	<dubbo:application name="${}"/>
	<!-- 用dubbo协议在20881端口暴露服务 -->
    <dubbo:protocol name="${}" port="${}"/>
    
    <!-- 需要暴露的服务接口 -->
    <dubbo:service interface="net.wangxj.ssm.service.share.AuthorityResourcesShareService" ref="authorityResourcesShareService"/>
    <bean id="authorityResourcesShareService" class="net.wangxj.ssm.service.share.impl.AuthorityResourcesShareServiceImpl"/>
   
   
</beans>