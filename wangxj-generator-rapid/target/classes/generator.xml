<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
	<comment>
		代码生成器配置文件: 
		1.会为所有的property生成property_dir属性,如pkg=com.company => pkg_dir=com/company
		2.可以引用环境变量: ${env.JAVA_HOME} or System.properties: ${user.home},property之间也可以相互引用
	</comment>
	<!-- 自定义变量 -->
	<entry key="author">wangxj</entry>
	
	<entry key="projectPreName">authority</entry>
	<entry key="projectGroupId">net.wangxj.authority</entry>
	
	<entry key="basepackage">net.wangxj.authority</entry>
	<!-- dao层变量 -->
	<entry key="databaseURLName">database.url</entry>
	<entry key="databaseURLValue">jdbc:mysql://localhost:3306/authority?useUnicode=true&amp;characterEncoding=UTF-8</entry>
	<entry key="compponentScanPackageName">component.scan.package</entry>
	<entry key="compponentScanPackageValue">net.wangxj.authority</entry>
	
	<entry key="databaseUSERName">database.username</entry>
	<entry key="databaseUSERValue">root</entry>
	
	<entry key="databasePASSWORDName">database.password</entry>
	<entry key="databasePASSWORDValue">llhua123</entry>
						
	<entry key="databaseMAXACTIVEName">database.maxActive</entry>
	<entry key="databaseMAXACTIVEValue">30</entry>
	
	<entry key="databaseVALIDATEQUERYName">database.validateQuery</entry>
	<entry key="databaseVALIDATEQUERYValue">select * from user</entry>
				 
	<entry key="aopPOINTEXPRESSIONName">database.aop.point.expression</entry>
	<entry key="aopPOINTEXPRESSIONValue">execution(* net.wangxj.authority.service..*(..))</entry>
	<!-- dao层变量结束 -->
	
	<!-- serviceimpl变量 -->
	
	<entry key="dubboAddressName">dubbo.address</entry>
	<entry key="dubboAddressValue">zookeeper://127.0.0.1:2181</entry>
	
	<entry key="dubboApplicationName">dubbo.application</entry>
	<entry key="dubboApplicationValue">authority_service</entry>
	
	<entry key="dubboProtocolName">dubbo.protocol</entry>
	<entry key="dubboProtocolValue">dubbo</entry>
	
	<entry key="dubboPortName">dubbo.port</entry>
	<entry key="dubbboPortValue">2881</entry>
	
	<!-- log -->
	<entry key="serviceLogRootName">authority.service.log.root</entry>
	<entry key="serviceLogRootValue">/home/huoshan/log</entry>
	
	<entry key="serviceLogNameName">authority.service.log.name</entry>
	<entry key="serviceLogNameValue">authority_service.log</entry>
	
	<entry key="serviceLogLevelName">authority.service.log.level</entry>
	<entry key="serviceLogLevelValue">DEBUG</entry>
	
	<entry key="serviceExceptionLogNameName">authority.service.exception.log</entry>
	<entry key="serviceExceptionLogNameValue">authority_service_exception.log</entry>
	
	<entry key="serviceSqlLogNameName">authority.service.sql.log.name</entry>
	<entry key="serviceSqlLogNameValue">authority_service_sql.log</entry>
	
	<entry key="serviceActiveMqLogName">authority.service.active.mq.log.name</entry>
	<entry key="serviceActiveMqLogNameValue">authority_service_activemq.log</entry>
	
	<!-- serviceimpl变量结束 -->
	
	<!-- web变量 -->
	<entry key="webFreemarkerDirName">authority.web.freemarker.dir</entry>
	<entry key="webFreemarkerDirValue"></entry>
	
	<entry key="webUploadMaxSizeName">authority.web.upload.maxsize</entry>
	<entry key="webUploadMaxSizeValue">800000000</entry>
	
	<entry key="webUploadErrorPageName">authority.web.upload.errorpage</entry>
	<entry key="webuploadErrorPageValue">error_page</entry>
	
	<entry key="webViewTemplateSuffixName">authority.web.view.template.suffix</entry>
	<entry key="webViewTemplateSuffixValue">.ftl</entry>
	
	
	<entry key="webLoggingRootName">authority.web.logging.root</entry>
	<entry key="webLoggingRootValue">/home/huoshan/log</entry>
	
	<entry key="webLoggingNameName">authority.web.logging.name</entry>
	<entry key="webLoggingNameValue">authority_web.log</entry>
	
	<entry key="webLoggingLevelName">authority.logging.level</entry>
	<entry key="webLoggingLevelValue">DEBUG</entry>
	
	<entry key="webDubboApplicationName">dubbo.application</entry>
	<entry key="webDubboApplicationValue">authority_web</entry>
	
	<!-- web变量结束 -->
	
	<!-- jsp namespace: web/${namespace}/${className}/list.jsp -->
	<entry key="namespace"></entry>

	<entry key="outRoot">/home/huoshan/generator-output</entry>
	
	<!-- 需要移除的表名前缀,使用逗号进行分隔多个前缀,示例值: t_,v_ -->
